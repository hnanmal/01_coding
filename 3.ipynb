    "def reduce(f, acc, list):\n",
    "    for a in list:\n",
    "        acc = f(acc, a)\n",
    "    return acc\n",
    "\n",
    "go = lambda a, *args: reduce(lambda a, f: f(a), a, args)\n",
    "\n",
    "go(10, lambda a: a + 10, lambda a: a + 1)"
   ]
